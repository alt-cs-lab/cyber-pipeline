{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "nodemon ./bin/www --ignore vue",
    "start": "NODE_ENV=production node ./bin/www",
    "reset": "knex migrate:rollback --all && knex migrate:latest && knex seed:run",
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "lint:check": "eslint .",
    "lint": "eslint --fix .",
    "test": "mocha --require test/hooks.js --recursive --timeout 15000 --exit",
    "cov": "nyc --reporter=html mocha --require test/hooks.js --recursive --timeout 2000 --exit",
    "test-json": "mocha --require test/hooks.js --recursive --timeout 2000 --exit --reporter json > test-results.json"
  },
  "dependencies": {
    "ajv-formats": "^3.0.1",
    "axios": "^1.7.7",
    "compression": "^1.7.4",
    "connect-history-api-fallback": "^2.0.0",
    "cookie-parser": "~1.4.6",
    "cors": "^2.8.5",
    "debug": "~4.3.7",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-mysql-session": "^3.0.3",
    "express-session": "^1.18.0",
    "helmet": "^7.1.0",
    "http-errors": "~2.0.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "morgan": "~1.10.0",
    "mysql": "^2.18.1",
    "node-cas-authentication": "github:alt-cs-lab/cas-authentication",
    "nodemon": "^3.1.4",
    "objection": "^3.1.4",
    "pug": "^3.0.3",
    "winston": "^3.14.2"
  },
  "devDependencies": {
    "chai": "^4.5.0",
    "chai-http": "^4.4.0",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "mocha": "^10.7.3",
    "prettier": "^3.3.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  }
}
